{
  "name": "chia11y",
  "private": true,
  "version": "1.0.0",
  "description": "A web dashboard for accessibility tests results.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:chialab/chia11y.git"
  },
  "scripts": {
    "start": "node packages/ws/index.js",
    "build": "npm run build-reporter && npm run build-devtools && npm run build-devtools-runner",
    "build-reporter": "rollup packages/pa11y-reporter-html/src/reporter.js --name 'Reporter' --file packages/pa11y-reporter-html/lib/reporter.js --config",
    "build-devtools": "rollup packages/devtools/src/popup/popup.js --name 'popup' --file packages/devtools/popup/popup.js --config",
    "build-devtools-runner": "rollup packages/devtools/src/runner.js --name 'popup' --file packages/devtools/runner.js --config"
  },
  "author": "Chialab",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "rollup": "^0.66.2",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-jst": "^1.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-string": "^2.0.2"
  }
}
