{
  "name": "Pa11y",
  "manifest_version": 2,
  "version": "1.0",
  "minimum_chrome_version": "10.0",
  "icons": {
    "128": "resources/icon.png"
  },
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "128": "resources/icon.png"
    }
  },
  "background": {
    "scripts": [
      "background/background.js"
    ]
  },
  "permissions": [
    "<all_urls>"
  ]
}